---
import Page from '../components/Page.astro'
import type { PageProps } from '../components/interfaces/PageProps'

const { frontmatter } = Astro.props
const { title, description, featuredImage } = frontmatter as PageProps
---

<Page title={title} description={description} featuredImage={featuredImage}>
  <h1>{title}</h1>
  <article class="post">
    <slot />
  </article>
</Page>
